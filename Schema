 create table charecter(
  charecter_name varchar,
  role_id int,
  lane_id int,
  char_id int
  );
  insert into charecter(charecter_name, role_id, lane_id, char_id) values
  ('roger', 1, 1, 1),
  ('mathilda', 2,2,2),
  ('harith',3,3,3),
  
  ('minotaur', 6, 2, 5),
  ('fredrinn',6,5,6),
  ('natan',4,1,7),
  
  ('Claude', 4, 1,9)
   ;
   
   create table char_role(
     role_name varchar,
     role_id int
     );
   insert into char_role(role_name,role_id) values
   ('fighter',1),
   ('support',2),
   ('mage',3),
   ('marksman',4),
   ('assasin',5),
   ('tank',6)
   ;
   
   create table lane(
     lane_name varchar,
     lane_id int
     );
   insert into lane(lane_name,lane_id) values
   ('gold',1),
   ('roam',2),
   ('mid',3),
   ('exp',4),
   ('jungle',5)
   ;
   
